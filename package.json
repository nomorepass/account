{
  "name": "account",
  "version": "0.1.0",
  "description": "a express application boilerplate",
  "main": "app/app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "NODE_ENV=dev nodemon app/server.js",
    "test": "rm -rf *.test.db && NODE_ENV=test mocha 'test/**/*.js'",
    "pretest": "npm run lint",
    "lint": "standard --env=mocha",
    "pretty": "prettier-standard 'app/**/*.js' 'test/**/*.js'",
    "coverage": "rm -rf *.test.db && NODE_ENV=test istanbul cover _mocha 'test/**/*.js'",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomorepass/account.git"
  },
  "keywords": [
    "express",
    "boilerplate"
  ],
  "author": "iSayme <isaymeorg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nomorepass/account/issues"
  },
  "homepage": "https://github.com/nomorepass/account#readme",
  "dependencies": {
    "body-parser": "^1.18.1",
    "config": "^1.26.2",
    "cors": "^2.8.4",
    "express": "^4.15.4",
    "express-partial-response": "^0.3.4",
    "morgan": "^1.8.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "sequelize": "^4.22.6",
    "sqlite3": "^3.1.13",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "co-mocha": "^1.2.0",
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^3.5.3",
    "nodemon": "^1.12.1",
    "power-assert": "^1.4.4",
    "prettier-standard": "^6.0.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0"
  }
}
